<%# Build The Content Heading %>
<% if (title) { %>
    <h1><%= title %></h1>
  <% } else {
    res.redirect('/')
  } %>

<%# Message Display %>
<%- messages() %>

<%# Errors Display %>
<% if (errors) { %>
  <ul class="notice">
 <% errors.array().forEach(error => { %>
   <li><%= error.msg %></li>
<%  }) %>
 </ul>
<% } %>

<%# Build The Details Display %>
<%- registerForm %>